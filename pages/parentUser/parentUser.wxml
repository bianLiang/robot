<!--miniprogram/pages/parentUser/parentUser.wxml-->
<view class="head">
  <cover-image style="width: 120rpx;height: 120rpx; position: absolute;left: 50%;margin-left: -60rpx;top: 40rpx;" src="../../images/touxiang.png"></cover-image>
  <text style="position: absolute;top:180rpx;left:50%;width:100rpx;margin-left:-50rpx;text-align: center;">登录</text>
</view>
<view style="background: #f8f8f8;height: 100%;">
  <view style="padding:24rpx;background: #fff;color:#333333;font-size:30rpx;">我的服务</view>
  <view class="item" style="border-bottom:1rpx solid #F2F2F2;">
    <view style="display: flex;">
      <cover-image style="width: 44rpx;height: 44rpx;" src="../../images/nai.png"></cover-image>
      <text style="margin-left:20rpx;">我的宝宝</text>
    </view>
    <mp-icon icon="arrow" color="#444" size="{{10}}" bindtap="goBaby"></mp-icon>
  </view>
  <view class="item">
    <view style="display: flex;">
      <cover-image style="width: 44rpx;height: 44rpx;" src="../../images/card.png"></cover-image>
      <text style="margin-left:20rpx;">绑定身份卡</text>
    </view>
    <mp-icon icon="arrow" color="#444" size="{{10}}" bindtap="goEditCard"></mp-icon>
  </view>
  <view class="item" style="margin-top:20rpx;">
    <view style="display: flex;">
      <cover-image style="width: 44rpx;height: 44rpx;" src="../../images/qiehuan.png"></cover-image>
      <text style="margin-left:20rpx;">切换角色</text>
    </view>
    <mp-icon icon="arrow" color="#444" size="{{10}}"></mp-icon>
  </view>
  <view class="sign-out">退出登录</view>
</view>

